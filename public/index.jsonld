            ],
            "authorList": [
                "https://julianrojas.org/#me",
                "https://data.verborgh.org/people/marina_aguado",
                "https://data.verborgh.org/people/polymnia_vasilopoulou",
                "https://data.verborgh.org/people/ivo_velitchkov",
                "https://dylanvanassche.be/#me",
